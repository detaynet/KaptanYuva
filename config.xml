<gap:splash src="www/res/screen/ios/Default-Landscape-736h@3x.png" gap:platform="ios" width="2208" height="1242" />

<!-- iPad -->
<gap:splash src="www/res/screen/ios/Default-Portrait.png" gap:platform="ios" width="768" height="1024" />
